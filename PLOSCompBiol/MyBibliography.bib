journal = {Mediators of Inflammation},
keywords = {GRN,HIF,HIF targets,hypoxia,network motif},
mendeley-tags = {GRN,HIF,HIF targets,hypoxia,network motif},
pages = {1--9},
title = {{The Transcription Factor ZNF395 Is Required for the Maximal Hypoxic Induction of Proinflammatory Cytokines in U87-MG Cells}},
