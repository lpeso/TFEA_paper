journal = {Thorax},
keywords = {80 and over,Activities of Daily Living,Adult,Aged,Chronic Obstructive,Chronic Obstructive: epidemiolo,Chronic Obstructive: psychology,Cost of Illness,Cross-Sectional Studies,Female,Humans,Male,Middle Aged,OSAS,Prevalence,Pulmonary Disease,Quality of Life,Risk Factors,Spain,Spain: epidemiology,review},
mendeley-tags = {OSAS,review},
month = {oct},
