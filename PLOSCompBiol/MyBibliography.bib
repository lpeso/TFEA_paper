journal = {Human genomics},
keywords = {Altitude,Anoxia,Anoxia: genetics,Anoxia: metabolism,Genetic,Genome-Wide Association Study,HIF,Humans,Indians,North American,North American: genetics,Polymorphism,Selection,Single Nucleotide,evolution,genetics,high altitude,hypoxia},
