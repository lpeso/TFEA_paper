file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yu et al. - 2012 - Regulation of cigarette smoke-mediated mucin expression by hypoxia-inducible factor-1$\alpha$ via epidermal growth factor r.pdf:pdf},
