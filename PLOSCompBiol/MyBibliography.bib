file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wu et al. - 2015 - Identifying differential transcription factor binding in ChIP-seq.pdf:pdf},
isbn = {1664-8021},
issn = {16648021},
journal = {Frontiers in Genetics},
keywords = {ChIP-seq,Differential binding,Methods comparison,Normalization,Validation,bioinformatics},
mendeley-tags = {ChIP-seq,bioinformatics},
number = {APR},
pages = {1--11},
pmid = {25972895},
title = {{Identifying differential transcription factor binding in ChIP-seq}},
volume = {6},
year = {2015}
}
