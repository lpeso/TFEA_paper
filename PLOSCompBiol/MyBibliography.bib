file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Eberl{\'{e}} et al. - 2004 - SREBP transcription factors Master regulators of lipid homeostasis.pdf:pdf},
isbn = {0012-1797 (Print)$\backslash$r0012-1797 (Linking)},
issn = {03009084},
journal = {Biochimie},
keywords = {Insulin,Lipids,SREBF,SREBP,Sterol regulatory element binding proteins (SREBPs,Transcription factors,teaching},
mendeley-tags = {SREBF,SREBP,teaching},
number = {11},
