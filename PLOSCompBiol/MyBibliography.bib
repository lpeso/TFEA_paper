file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fu et al. - 2011 - Generation of a mouse model of Von Hippel-Lindau kidney disease leading to renal cancers by expression of a constitu.pdf:pdf},
isbn = {1527-3792 (Electronic)$\backslash$r0022-5347 (Linking)},
issn = {00085472},
journal = {Cancer Research},
keywords = {HIF,VHL,cancer},
mendeley-tags = {HIF,VHL,cancer},
number = {21},
pages = {6848--6856},
pmid = {21908555},
title = {{Generation of a mouse model of Von Hippel-Lindau kidney disease leading to renal cancers by expression of a constitutively active mutant of HIF1$\alpha$}},
