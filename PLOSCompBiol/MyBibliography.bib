journal = {The Journal of biological chemistry},
keywords = {Cell Line,Cysteine Endopeptidases,Cysteine Endopeptidases: metabolism,DNA-Binding Proteins,DNA-Binding Proteins: metabolism,HIF,Hydrolysis,Hypoxia-Inducible Factor 1,Multienzyme Complexes,Multienzyme Complexes: metabolism,Nuclear Proteins,Nuclear Proteins: metabolism,Oxidation-Reduction,Protease Inhibitors,Protease Inhibitors: pharmacology,Proteasome Endopeptidase Complex,Transcription Factors,Transcription Factors: metabolism,Ubiquitins,Ubiquitins: metabolism,alpha Subunit},
