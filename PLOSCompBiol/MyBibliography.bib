journal = {Current genetics},
keywords = {Corepressor,Cyc8,Ino2,Pho4,Saccharomyces cerevisiae,Sin3,Transcriptional activator,corepressor,repression,sin3a,trancription,transcription},
mendeley-tags = {corepressor,repression,sin3a,trancription,transcription},
