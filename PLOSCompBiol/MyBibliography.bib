title = {{Heterogeneous Effects of Direct Hypoxia Pathway Activation in Kidney Cancer}},
url = {http://dx.plos.org/10.1371/journal.pone.0134645 http://www.ncbi.nlm.nih.gov/pubmed/26262842 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4532367},
