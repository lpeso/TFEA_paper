issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Differentially expressed genes,HIF,c. elegans,gene profiling,hypoxia,microarray,vhl},
mendeley-tags = {Differentially expressed genes,HIF,c. elegans,gene profiling,hypoxia,microarray,vhl},
month = {may},
