doi = {10.1093/bib/5.3.217},
file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pavesi, Mauri, Pesole - 2004 - In silico representation and discovery of transcription factor binding sites.pdf:pdf},
isbn = {1467-5463 (Print)},
issn = {1467-5463},
journal = {Briefings in bioinformatics},
keywords = {annotation,bioinformatics,genome,motif discovery,position-specific,promoter,review,software,software tools,teaching,transcription factor binding site,weight matrix},
