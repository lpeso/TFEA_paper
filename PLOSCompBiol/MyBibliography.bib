issn = {10196439},
journal = {International Journal of Oncology},
keywords = {Antiangiogenic treatment,Hypoxia,My{\_}Publications,My{\{}{\_}{\}}Publications,Optical imaging,Ovarian carcinoma,Xenograft,my papers},
