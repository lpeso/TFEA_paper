number = {Database issue},
pages = {D262--6},
pmid = {14681408},
title = {{The Gene Ontology Annotation (GOA) Database: sharing knowledge in Uniprot with Gene Ontology.}},
volume = {32},
year = {2004}
}
@article{Kadamb2013,
