file = {:home/luis/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Morote-Garcia et al. - 2008 - HIF-1-dependent repression of adenosine kinase attenuates hypoxia-induced vascular leak.pdf:pdf},
issn = {1528-0020},
journal = {Blood},
