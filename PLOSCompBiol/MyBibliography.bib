journal = {BMC genomics},
keywords = {Animal Genetics and Genomics,Life Sciences,Microarrays,Microbial Genetics and Genomics,Plant Genetics {\&} Genomics,Proteomics,corepressor,general,repression,sin3a,trancription,transcription},
