issn = {00085472},
journal = {Cancer Research},
keywords = {HIF,epigenetics,hypoxia,methylation},
mendeley-tags = {HIF,epigenetics,hypoxia,methylation},
number = {18},
pages = {9009--9016},
pmid = {16982742},
title = {{Hypoxic stress induces dimethylated histone H3 lysine 9 through histone methyltransferase G9a in mammalian cells}},
volume = {66},
year = {2006}
