issn = {0021-9258},
journal = {The Journal of biological chemistry},
month = {jul},
number = {28},
pages = {29013--22},
pmid = {15123621},
title = {{The aryl hydrocarbon receptor displaces p300 from E2F-dependent promoters and represses S phase-specific gene expression.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15123621},
volume = {279},
year = {2004}
